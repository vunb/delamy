{
  "productName": "Meetings",
  "directories": {
    "output": "release/"
  },
  "files": [
      "dist/**/*",
      "main.js",
      "package.json",
      "!**/*.ts",
      "!*.code-workspace",
      "!LICENSE.md",
      "!package-lock.json",
      "!src/",
      "!e2e/",
      "!hooks/",
      "!angular.json",
      "!_config.yml",
      "!karma.conf.js",
      "!tsconfig.json",
      "!tslint.json"
  ],
  "win": {
    "icon": "builder/icons/win/icon.ico",
    "target": {
      "target": "msi",
      "arch": [
        "x64",
        "ia32"
      ]
    }
  },
  "mac": {
    "icon": "dist",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  }
}
